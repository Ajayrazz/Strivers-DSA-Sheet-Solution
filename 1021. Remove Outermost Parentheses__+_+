class Solution {
    public String removeOuterParentheses(String s) {
        Stack<Character> stack = new Stack<>();
        StringBuilder sb = new StringBuilder();

        for(char ch : s.toCharArray()){
            if(ch == '('){
                if(!stack.isEmpty()){
                    sb.append(ch);
                }
â€¦
        return sb.toString();
    }
}
